{
  "presets": [
    [
      "babel-preset-react-app",
      {
        "targets": {
          "node": "current"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "overrides": [
    {
      "include": "modules/",
      "plugins": [
        "@mm/babel-plugin-transform-mm2"
      ]
    },
    {
      "include": "config/",
      "plugins": [
        "@mm/babel-plugin-transform-config"
      ]
    }
  ],
  "sourceMaps": true
}